{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "github-jobs",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "github-jobs": {
      "root": "apps/github-jobs",
      "sourceRoot": "apps/github-jobs/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/github-jobs",
            "index": "apps/github-jobs/src/index.html",
            "main": "apps/github-jobs/src/main.tsx",
            "polyfills": "apps/github-jobs/src/polyfills.ts",
            "tsConfig": "apps/github-jobs/tsconfig.app.json",
            "assets": [
              "apps/github-jobs/src/favicon.ico",
              "apps/github-jobs/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/github-jobs/src/environments/environment.ts",
                  "with": "apps/github-jobs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "github-jobs:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "github-jobs:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/github-jobs/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/github-jobs"],
          "options": {
            "jestConfig": "apps/github-jobs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "github-jobs-e2e": {
      "root": "apps/github-jobs-e2e",
      "sourceRoot": "apps/github-jobs-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/github-jobs-e2e/cypress.json",
            "tsConfig": "apps/github-jobs-e2e/tsconfig.e2e.json",
            "devServerTarget": "github-jobs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "github-jobs:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/github-jobs-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-ui-component": {
      "root": "libs/shared/ui/component",
      "sourceRoot": "libs/shared/ui/component/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/component/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui/component"],
          "options": {
            "jestConfig": "libs/shared/ui/component/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui-theme": {
      "root": "libs/shared/ui/theme",
      "sourceRoot": "libs/shared/ui/theme/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/theme/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui/theme"],
          "options": {
            "jestConfig": "libs/shared/ui/theme/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "use-case-filter-jobs": {
      "root": "libs/use-case/filter-jobs",
      "sourceRoot": "libs/use-case/filter-jobs/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/use-case/filter-jobs/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/use-case/filter-jobs"],
          "options": {
            "jestConfig": "libs/use-case/filter-jobs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "domain-jobs": {
      "root": "libs/domain/jobs",
      "sourceRoot": "libs/domain/jobs/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/domain/jobs/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domain/jobs"],
          "options": {
            "jestConfig": "libs/domain/jobs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
